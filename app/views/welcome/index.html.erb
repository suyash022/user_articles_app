<h1>Blog App</h1>
<h1>Listing Articles</h1>
<table>
  <tr>
    <th>Author</th>
    <th>Title</th>
    <th>Email</th>
    <th colspan="3"></th>
  </tr>

  <% @articles.each do |article| %>
    <tr>
      <td><%= article.user.email %></td>
      <td><%= article.author %></td>
      <td><%= article.title %></td>
      <td><%= link_to 'Show', user_article_path(article.user, article) %></td>
    </tr>
  <% end %>
</table>